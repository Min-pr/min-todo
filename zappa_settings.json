{
    "dev": {
        "app_function": "wsgi.app",
        "aws_region": "ap-northeast-2",
        "profile_name": "default",
        "project_name": "min-todo",
        "runtime": "python3.9",
        "s3_bucket": "min-todo-zappa",
        "environment_variables": {
            "FLASK_TEMPLATE_ENV": "dev"
          },
          "cors": true,
          "log_level": "WARN",
          "extra_permissions": [
            {
              "Effect": "Allow",
              "Action": [
                "ssm:GetParameter"
              ],
              "Resource": "*"
              }
          ],

          "num_retained_versions": 1
      }
  }

